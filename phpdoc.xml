<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
    configVersion="3"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://www.phpdoc.org"
    xsi:schemaLocation="https://www.phpdoc.org https://docs.phpdoc.org/latest/phpdoc.xsd"
>
    <title>VISU - PHP Game Framework</title>
    <paths>
        <output>docs/api</output>
        <cache>var/cache/phpdoc</cache>
    </paths>
    <version number="latest">
        <api>
            <source dsn=".">
                <path>src</path>
            </source>
            <ignore>
                <path>vendor/**/*</path>
                <path>tests/**/*</path>
                <path>examples/**/*</path>
            </ignore>
        </api>
    </version>
    <setting name="graphs.enabled" value="true"/>
    <setting name="guides.enabled" value="true"/>
</phpdocumentor>
